#compiles and zips lab6 
all:lab6 lab6.zip

#removes old lab6
clean: lab6
	rm lab6

#compile code into .o file
lab6:
	gcc lab6.s lab6main.c -g -o lab6

#zips code
lab6.zip: lab6main.c
	zip lab6.zip lab6.s lab6main.c makefile
